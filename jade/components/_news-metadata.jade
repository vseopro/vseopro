-var user = ['Administrator', 'TDSko', 'Pasha', 'Sasha', 'SuperMegaDuperUser3000', 'Terminator T3000']
-var tag = ['Фотошоп', 'Photohop', 'Илюстратор', 'Длинныйтекст', 'Оч Длинный текст']

.row-mini.news-metadata
  .col-sm-4.col-md-4
    if dev
      a(href="#").link.news-metadata__text.text-overflow <span class="icon-user news-metadata__ico"></span> #{user[Math.floor(Math.random() * ((user.length - 1) - 0 + 1)) + 0]}
    else
      a(href="#").link.news-metadata__text.text-overflow <span class="icon-user news-metadata__ico"></span> Admin
  .col-sm-4.col-md-4
    if dev
      a(href="#").link.news-metadata__text.text-overflow <span class="icon-calendar news-metadata__ico"></span> 0#{Math.floor(Math.random() * (9 - 1 + 1)) + 1}.0#{Math.floor(Math.random() * (9 - 1 + 1)) + 1}.#{Math.floor(Math.random() * (2100 - 2000 + 1)) + 2000}
    else
      a(href="#").link.news-metadata__text.text-overflow <span class="icon-calendar news-metadata__ico"></span> 01.02.2010
  .col-sm-4.col-md-4
    if dev
      a(href="#" data-toggle="tooltip" data-placement="top" title="Тестовый текст").link.news-metadata__text.text-overflow <span class="icon-folder-open news-metadata__ico"></span> #{tag[Math.floor(Math.random() * ((tag.length - 1) - 0 + 1)) + 0]}
    else
      a(href="#" data-toggle="tooltip" data-placement="top" title="Тестовый текст").link.news-metadata__text.text-overflow <span class="icon-folder-open news-metadata__ico"></span> Photoshop
